<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin" tools:context=".MainActivity"
    android:id="@+id/relativeLayout"
    android:background="@drawable/gradient"
    android:clickable="false">



    <RelativeLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:id="@+id/relativeLayout3"
        android:gravity="center"
        android:layout_alignParentLeft="true">

        <!-- Clock wise - 270 -->
        <com.music.android.widget.VerticalSeekBarWrapper
            android:id="@+id/seekbar_vertical_volume_container"
            android:layout_width="wrap_content"
            android:layout_height="@dimen/xx_seekbar_height"
            android:layout_toLeftOf="@+id/seekbar_vertical_1_container"
            android:layout_marginRight="35dp">
            <com.music.android.widget.VerticalSeekBar
                android:id="@+id/seekbar_vertical_volume"
                app:seekBarRotation="CW270"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:splitTrack="false" />
        </com.music.android.widget.VerticalSeekBarWrapper>

        <com.music.android.widget.VerticalSeekBarWrapper
            android:id="@+id/seekbar_vertical_1_container"
            android:layout_width="wrap_content"
            android:layout_height="@dimen/xx_seekbar_height"
            android:layout_toLeftOf="@+id/seekbar_vertical_2_container"
            android:layout_marginRight="@dimen/xx_seekbar_right_margin">
            <com.music.android.widget.VerticalSeekBar
                android:id="@+id/seekbar_vertical_1"
                android:layout_width="0dp"
                android:layout_height="0dp"
                app:seekBarRotation="CW270" />
        </com.music.android.widget.VerticalSeekBarWrapper>

        <com.music.android.widget.VerticalSeekBarWrapper
            android:id="@+id/seekbar_vertical_2_container"
            android:layout_width="wrap_content"
            android:layout_height="@dimen/xx_seekbar_height"
            android:layout_toLeftOf="@+id/seekbar_vertical_3_container"
            android:layout_marginRight="@dimen/xx_seekbar_right_margin">
            <com.music.android.widget.VerticalSeekBar
                android:id="@+id/seekbar_vertical_2"
                android:layout_width="0dp"
                android:layout_height="0dp"
                app:seekBarRotation="CW270"/>
        </com.music.android.widget.VerticalSeekBarWrapper>

        <com.music.android.widget.VerticalSeekBarWrapper
            android:id="@+id/seekbar_vertical_3_container"
            android:layout_width="wrap_content"
            android:layout_height="@dimen/xx_seekbar_height"
            android:layout_toLeftOf="@+id/seekbar_vertical_4_container"
            android:layout_marginRight="@dimen/xx_seekbar_right_margin" >
            <com.music.android.widget.VerticalSeekBar
                android:id="@+id/seekbar_vertical_3"
                android:layout_width="0dp"
                android:layout_height="0dp"
                app:seekBarRotation="CW270" />
        </com.music.android.widget.VerticalSeekBarWrapper>


        <com.music.android.widget.VerticalSeekBarWrapper
            android:id="@+id/seekbar_vertical_4_container"
            android:layout_width="wrap_content"
            android:layout_height="@dimen/xx_seekbar_height"
            android:layout_toLeftOf="@+id/seekbar_vertical_5_container"
            android:layout_marginRight="@dimen/xx_seekbar_right_margin" >
            <com.music.android.widget.VerticalSeekBar
                android:id="@+id/seekbar_vertical_4"
                android:layout_width="0dp"
                android:layout_height="0dp"
                app:seekBarRotation="CW270"/>
        </com.music.android.widget.VerticalSeekBarWrapper>

        <com.music.android.widget.VerticalSeekBarWrapper
            android:id="@+id/seekbar_vertical_5_container"
            android:layout_width="wrap_content"
            android:layout_height="@dimen/xx_seekbar_height"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="false"
            android:layout_alignParentRight="true">

            <com.music.android.widget.VerticalSeekBar
                android:id="@+id/seekbar_vertical_5"
                android:layout_width="0dp"
                android:layout_height="0dp"
                app:seekBarRotation="CW270"/>
        </com.music.android.widget.VerticalSeekBarWrapper>
        <!--<include-->
        <!--android:id="@+id/include1"-->
        <!--layout="@layout/action_bar" />-->

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:text="14K"
            android:id="@+id/textView"
            android:layout_below="@+id/seekbar_vertical_5_container"
            android:layout_alignLeft="@+id/seekbar_vertical_5_container"
            android:layout_alignRight="@+id/seekbar_vertical_5_container"
            android:gravity="center" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:text="3K"
            android:id="@+id/textView5"
            android:layout_alignTop="@+id/textView"
            android:layout_alignLeft="@+id/seekbar_vertical_4_container"
            android:layout_alignRight="@+id/seekbar_vertical_4_container"
            android:gravity="center" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:text="910"
            android:id="@+id/textView6"
            android:layout_alignTop="@+id/textView5"
            android:layout_alignLeft="@+id/seekbar_vertical_3_container"
            android:layout_alignRight="@+id/seekbar_vertical_3_container"
            android:gravity="center" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:text="230"
            android:id="@+id/textView7"
            android:layout_alignRight="@+id/seekbar_vertical_2_container"
            android:gravity="center"
            android:layout_alignLeft="@+id/seekbar_vertical_2_container"
            android:layout_below="@+id/seekbar_vertical_2_container" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:text="60"
            android:id="@+id/textView8"
            android:layout_alignRight="@+id/seekbar_vertical_1_container"
            android:gravity="center"
            android:layout_alignBottom="@+id/textView"
            android:layout_alignLeft="@+id/seekbar_vertical_1_container" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:text="Vol."
            android:id="@+id/textView9"
            android:gravity="center"
            android:layout_below="@+id/seekbar_vertical_volume_container"
            android:layout_alignLeft="@+id/seekbar_vertical_volume_container"
            android:layout_alignRight="@+id/seekbar_vertical_volume_container" />

    </RelativeLayout>

    <RelativeLayout
        android:orientation="vertical"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/relativeLayout2"
        android:layout_alignParentBottom="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentEnd="false"
        android:layout_alignWithParentIfMissing="false"
        android:layout_alignTop="@+id/relativeLayout3">

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/imageView"
            android:layout_centerVertical="true"

            android:layout_alignLeft="@+id/frameLayout"
            android:layout_alignStart="@+id/frameLayout" />

        <FrameLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:id="@+id/frameLayout">

            <com.sakuramomoko.searchinganimview.SearchingAnimView
                android:id="@+id/searchinganimview"
                app:search_color="#fff"
                android:layout_width="214dp"
                android:onClick="onClickAnim"
                android:layout_height="216dp" />

            <TextView
                android:id="@+id/tv_searching_state"
                android:layout_width="150dp"
                android:layout_height="150dp"
                android:layout_gravity="center"
                android:background="@drawable/slice"
                android:textSize="20dp" />

        </FrameLayout>

        <!--<Button-->
        <!--android:id="@+id/btUpgrade"-->
        <!--android:layout_width="wrap_content"-->
        <!--android:layout_height="wrap_content"-->
        <!--android:onClick="onBtUpgradeClick"-->
        <!--android:text="@string/upgrade_to_premium"-->
        <!--android:layout_alignParentBottom="true"-->
        <!--android:layout_centerHorizontal="true" />-->

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView2"
            android:layout_centerHorizontal="true"
            android:layout_below="@+id/frameLayout" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Enroll NOW!"
            android:textSize="18sp"
            android:id="@+id/textView4"
            android:onClick="onBtUpgradeClick"
            android:layout_below="@+id/textView2"
            android:layout_centerHorizontal="true" />

        <ImageView
            android:layout_width="40dp"
            android:layout_height="40dp"
            app:srcCompat="@drawable/fb_icon"
            android:layout_alignParentBottom="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true"
            android:id="@+id/fb" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView10"
            android:layout_alignParentBottom="true"
            android:layout_toRightOf="@+id/equalizer_view"
            android:layout_toEndOf="@+id/equalizer_view"
            android:layout_marginLeft="13dp"
            android:layout_marginStart="13dp" />

        <es.claucookie.miniequalizerlibrary.EqualizerView
            android:id="@+id/equalizer_view"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:background="#00000000"
            app:animDuration="3500"
            app:foregroundColor="0xFFFFFFFF"
            android:layout_alignParentBottom="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" />

        <TextView
            android:text="3D Sound is OFF "
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/status"
            android:layout_alignParentBottom="true"
            android:layout_toLeftOf="@+id/fb"
            android:layout_alignLeft="@+id/textView10"
            android:layout_alignStart="@+id/textView10"
            android:layout_alignTop="@+id/textView10" />

    </RelativeLayout>


    <!-- <com.google.android.gms.plus.PlusOneButton
         xmlns:plus="http://schemas.android.com/apk/lib/com.google.android.gms.plus"
         android:id="@+id/plus_one_button"
         android:layout_width="wrap_content"
         android:layout_height="wrap_content"
         plus:size="standard"
         plus:annotation="inline"/>
 !-->
</RelativeLayout>
